<div class="row-fluid">
    <div class="box span12">
        <div class="box-header well" data-original-title>
            <h2>
                <i class="icon-list-alt"></i>&nbsp;
                Create Structures
            </h2>
        </div>
        <div class="box-content">
            <% for(var i in GizmoCore.navs['MENU_ADMIN_STRUCTURE']) { %>
            <a href="<%=GizmoCore.navs['MENU_ADMIN_STRUCTURE'][i].link%>" class="btn btn-info">
                <i class="icon-plus icon-white"></i>
                <%=GizmoCore.navs['MENU_ADMIN_STRUCTURE'][i].title%>
            </a>
            <% } %>
        </div>
    </div>
</div>
<div class="row-fluid">
    <div class="box span12">
        <div class="box-header well" data-original-title>
            <h2>
                <i class="icon-tasks"></i>
                Content Type
            </h2>
        </div>
        <div class="box-content">
            <table class="table table-striped table-bordered">
                <thead>
                <tr>
                    <th>Content Type</th>
                    <th>Machine Name</th>
                    <th>description</th>
                    <th>Operations</th>
                </tr>
                </thead>
                <tbody>
                <% for (var i in contentTypes) { %>
                <tr>
                    <td><%=contentTypes[i].title.text%></td>
                    <td><%=contentTypes[i].machine_name%></td>
                    <td><%=contentTypes[i].description%></td>
                    <td>
                        <a href="/admin/content/<%=contentTypes[i]._id%>/edit" class="btn btn-success">
                            <i class="icon-edit icon-white"></i>
                            Edit
                        </a>
                        <a href="/" class="btn btn-danger">
                            <i class="icon-trash icon-white"></i>
                            Remove
                        </a>
                    </td>
                </tr>
                <% } %>
                </tbody>
            </table>
        </div>
    </div>
</div>
