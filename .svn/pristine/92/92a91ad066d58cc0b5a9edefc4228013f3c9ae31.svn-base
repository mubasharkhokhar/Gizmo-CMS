
<div class="row-fluid">
    <div class="box span12">
        <div class="box-header well" data-original-title>
            <h2>
                <i class="icon-user"></i>
                Create Node
            </h2>
        </div>
        <div class="box-content">
            <form action="" method="post" class="form-horizontal use-ajax-submit">
                <fieldset>
                    <div class="control-group">
                        <label class="control-label">Title</label>
                        <div class="controls">
                            <input type="text" name="title" class="span6 typeahead" required="1"/>
                        </div>
                    </div>
                    <div class="control-group">
                        <label class="control-label">Tags</label>
                        <div class="controls">
                            <input type="text" name="tags" class="span6 typeahead" required="1"/>
                        </div>
                    </div>
                    <div class="control-group">
                        <label class="control-label">Description</label>
                        <div class="controls">
                            <textarea name="description" required="1" class="cleditor"></textarea>
                        </div>
                    </div>
                    <div class="control-group">
                        <label class="control-label">Alias</label>
                        <div class="controls">
                            <input type="text" name="alias" class="span6 typeahead" required="1"/>
                        </div>
                    </div>
                    <div class="form-actions">
                        <input type="submit" value="Save Changes" class="btn btn-primary"/>
                        <input type="reset" value="Cancel" class="btn"/>
                    </div>
                </fieldset>
            </form>
        </div>
    </div>
</div>
