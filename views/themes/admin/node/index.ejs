<div class="row-fluid">
    <div class="box span12">
        <div class="box-header well" data-original-title>
            <h2>
                <i class="icon-user"></i>
                Select Content Type
            </h2>
        </div>
        <div class="box-content">
            <% for(var i in contentTypes) { %>
            <a href="/node/<%=contentTypes[i].machine_name%>/create" class="btn btn-info">
                <i class="icon-plus icon-white"></i>
                <%=contentTypes[i].title.text%>
            </a>
            <% } %>
        </div>
    </div>
</div>